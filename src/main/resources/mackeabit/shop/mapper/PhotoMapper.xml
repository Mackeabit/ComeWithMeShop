<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mackeabit.shop.mapper.PhotoMapper">

    <insert id="saveReviewPhoto" parameterType="mackeabit.shop.vo.ReviewsVO" useGeneratedKeys="true" keyProperty="img_idx">
        insert into photos values(0, #{member_idx}, null, #{review_idx}, #{img_name}, #{file_name})
    </insert>






</mapper>