<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mackeabit.shop.mapper.PaymentMapper">

    <insert id="save" parameterType="map" useGeneratedKeys="true" keyProperty="pay_idx">
        insert into payments values(0, #{order_mi}, #{member_idx}, #{total_price}, -1, #{pay_code}, #{shipping_code}, now())
    </insert>






</mapper>